/*
 * 国際化関連のメソッドを集めたクラス
 * 初期値はデフォルトの言語設定(JP)がセットされる
 */
public with sharing class AtkI18n {
	private static Map<String,Object> messageTable = initMessageTable();
	private static final String Q = String.fromCharArray(new Integer[]{39});
	private static final String MASTER_LOCALE = 'ja';
	private static String locale = MASTER_LOCALE;
	private static final Time TIME_ZERO = Time.newInstance(0,0,0,0);
	private static final AtkCommonLogic common = new AtkCommonLogic();

   // ロケールを切り替える
	public static void setLocale(String loc) {
		if(loc == null)
			loc = TxsUtil.getUserLocale();
		locale = loc;
	}

	public static Map<String,String> getMapMessage(String loc) {
		if(loc == 'en' || loc == 'en_US')
			return getMessagesEn();
		else
			return getMessagesJa();
	}

	private static Map<String,Object> initMessageTable()
	{
			List<AtkCommon__c> commons = [select MessageTable__c, PlugJavaScript__c from AtkCommon__c limit 1];
			String message = null;
			if(!commons.isEmpty()){
			message = commons[0].MessageTable__c;
		}
		if(message == null || message == '+Q+'){
			message = '{}';
		}
		return (Map<String,Object>)JSONUtil.deserialize(message);
	}

	// 指定した メッセージIDのメッセージを返す
	public static String getMessage(String loc, String messageId) {
		Map<String,Object> localMessageMap = (Map<String,Object>)messageTable.get(loc);
		//message table 未定義

		String message = null;

		if(localMessageMap != null)
			message = (String)localMessageMap.get(messageId);

		if(message == null) {
			Map<String, String> mapMsg = getMapMessage(loc);
			if(mapMsg != null)
				message = mapMsg.get(messageId);
		}
		return message == null ? 'Error ' + messageId : message;
	}

	// 指定した メッセージIDのメッセージを返す
	public static String getMessage(String messageId) {
		return getMessage(locale, messageId);
	}

	// 指定した メッセージIDのメッセージを返す
	public static String getMessageMaster(String messageId) {
		AtkCommon__c c = common.load();
		String mid = messageId;
		if(messageId.substring(0,2) == 'zv' && c.NamingRule__c == '1') {
			mid = 'zw' + messageId.substring(2);
		}
		return getMessage(MASTER_LOCALE, mid);
	}

	public static String getMessage(String messageId, List<String> args)
	{
		return String.format(getMessage(messageId), args);
	}

	public static String getMessage(String loc, String messageId, List<String> args)
	{
		return String.format(getMessage(loc, messageId), args);
	}

	public static String getMessageMaster(String messageId, List<String> args)
	{
		return String.format(getMessageMaster(messageId), args);
	}

	public static String formatYMD(Date d) {
		if(d == null) return '';
		String m = getMessage('zv00000010');
		DateTime dt = DateTime.newInstance(d, TIME_ZERO);
		return dt.format(m);
	}

	public static String formatYMD(String loc, Date d) {
		if(d == null) return '';
		String m = getMessage(loc, 'zv00000010');
		DateTime dt = DateTime.newInstance(d, TIME_ZERO);
		return dt.format(m);
	}

	public static String formatYM(Date d, Integer sub) {
		if(d == null) return '';
		String m = getMessage('zv00000020');
		DateTime dt = DateTime.newInstance(d, TIME_ZERO);
		String s = dt.format(m);
		if(sub != null)
			s = getMessage('zv00000025', new String[]{s, String.valueOf(sub+1)});
		return s;
	}

	public static String formatYM(String loc, Date d, Integer sub) {
		if(d == null) return '';
		String m = getMessage(loc, 'zv00000020');
		DateTime dt = DateTime.newInstance(d, TIME_ZERO);
		String s = dt.format(m);
		if(sub != null)
			s = getMessage(loc, 'zv00000025', new String[]{s, String.valueOf(sub+1)});
		return s;
	}

	public static String formatYM(Integer ym, Integer sub) {
		return formatYM(Date.newInstance(ym/100, Math.mod(ym,100), 1), sub);
	}

	public static String formatYM(String loc, Integer ym, Integer sub) {
		return formatYM(loc, Date.newInstance(ym/100, Math.mod(ym,100), 1), sub);
	}

	public static String formatYMDHM(DateTime dt) {
		if(dt == null) return '';
		String m = getMessage('zv00000030');
		return dt.format(m);
	}

	public static String formatYMDHM(String loc, DateTime dt) {
		if(dt == null) return '';
		String m = getMessage(loc, 'zv00000030');
		return dt.format(m);
	}

	public static String formatMasterYMD(Date d) {
		if(d == null) return '';
		String m = getMessageMaster('zv00000010');
		DateTime dt = DateTime.newInstance(d, TIME_ZERO);
		return dt.format(m);
	}

	public static String formatMasterYM(Date d, Integer sub) {
		if(d == null) return '';
		String m = getMessageMaster('zv00000020');
		DateTime dt = DateTime.newInstance(d, TIME_ZERO);
		String s = dt.format(m);
		if(sub != null)
			s = getMessageMaster('zv00000025', new String[]{s, String.valueOf(sub+1)});
		return s;
	}

	public static String formatMasterYM(Integer ym, Integer sub) {
		return formatMasterYM(Date.newInstance(ym/100, Math.mod(ym,100), 1), sub);
	}

	public static String getYoubi(Integer y) {
		return getMessage('zv0000006'+y);
	}

	public static String getYoubiMaster(Integer y) {
		return getMessageMaster('zv0000006'+y);
	}

	public static String getYoubi(Date d) {
		return getYoubi(AtkUtil.getDayOfWeek(d));
	}

	public static String getYoubiMaster(Date d) {
		return getYoubiMaster(AtkUtil.getDayOfWeek(d));
	}

